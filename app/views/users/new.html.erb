<%= form_for @user do |f| %> 
    <%= f.label :name, "User Name" %> 
    <%= f.text_field :name %> 
    <%= f.label :password, "Password" %> 
    <%= f.password_field :password %>
    <%= f.label :height%> 
    <%= f.number_field :height %>
    <%= f.label :nausea %> 
    <%= f.number_field :nausea %>
    <%= f.label :tickets %> 
    <%= f.number_field :tickets %>
    <%= f.label :happiness %> 
    <%= f.number_field :happiness %>
    <%= f.submit "Create User" %>
    <% end %>
